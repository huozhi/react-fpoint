{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "declaration": true,
    "sourceMap": false,
    "outDir": "./dist",
    "removeComments": true,
    "moduleResolution": "Node",
    "strict": true,
    "typeRoots": [
      "./@types",
      "./node_modules/@types"
    ],
    "types": [
      "react",
      "fpoint"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,

    "forceConsistentCasingInFileNames": true
  }
}
