{
  "name": "react-fpoint",
  "version": "0.1.0",
  "description": "react components for fpoint",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --outDir ./dist/cjs --module CommonJS",
    "build:esm": "tsc --outDir ./dist/esm --module ESNext"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/huozhi/react-fpoint.git"
  },
  "keywords": [
    "react",
    "gesture",
    "mouse",
    "touch"
  ],
  "author": "huozhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huozhi/react-fpoint/issues"
  },
  "homepage": "https://github.com/huozhi/react-fpoint#readme",
  "dependencies": {
    "fpoint": "0.2.8",
    "typescript": "4.0.5"
  },
  "peerDependencies": {
    "react": "^16.11.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "16.9.55",
    "react": "16",
    "react-dom": "16"
  }
}
